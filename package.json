{
  "name": "mia-hot-start",
  "version": "0.4.1",
  "description": "The Mia Platform Fastify plugin",
  "main": "src/index.js",
  "types": "src/types/index.d.ts",
  "scripts": {
    "test": "npm run lint && npm run unit",
    "unit": "node --test --experimental-test-coverage test/*.test.js test/**/*.test.js",
    "collect-coverage": "NODE_OPTIONS='--test-reporter lcov --test-reporter-destination=lcov.info' npm run unit",
    "lint": "eslint . --ignore-path=.gitignore",
    "example": "node example/index.js",
    "spellcheck": "cspell '**/*.md'"
  },
  "keywords": [
    "Fastify",
    "Mia"
  ],
  "author": "Edoardo Crippa",
  "license": "Apache-2.0",
  "devDependencies": {
    "@mia-platform/eslint-config-mia": "^3.0.0",
    "eslint": "^8.56.0",
    "form-auto-content": "^3.2.1",
    "nock": "^13.5.0",
    "cspell": "^8.7.0"
  },
  "dependencies": {
    "deepmerge": "^4.3.1"
  },
  "peerDependencies": {
    "@fastify/env": ">= 4.3.0",
    "@fastify/formbody": ">= 7.4.0",
    "@fastify/swagger": ">= 8.12.2",
    "@fastify/swagger-ui": ">= 2.1.0",
    "axios": ">= 1.6.7",
    "fastify": ">= 4.26.0",
    "fastify-metrics": ">= 10.6.0",
    "fastify-plugin": ">= 4.5.1",
    "pino": ">= 8.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
